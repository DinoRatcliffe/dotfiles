def_key "j"
    scroll_down
def_key "k"
    scroll_up
def_key "h"
    previous_column
def_key "l"
    next_column

def_key "."
    show_lyrics

def_key "t"
    show_tag_editor
def_key "m"
    show_media_library
def_key "m"
    toggle_media_library_columns_mode
def_key "v"
    show_visualizer

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

def_key "J"
    move_sort_order_down
def_key "K"
    move_sort_order_up
